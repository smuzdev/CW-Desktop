<Page x:Class="KlingenRestaurant.Pages.Menu.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KlingenRestaurant.Pages.Menu"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SettingsPage"
      DataContext="{Binding Path=SettingsViewModel, Source={StaticResource Locator}}">
    
    <Grid>
        <TextBlock Text="{Binding SettingsPageText}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25"/>
    </Grid>
</Page>
